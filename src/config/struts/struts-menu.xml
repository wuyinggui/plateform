<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="menu" namespace="/menu" extends="json-default,default">
		<action name="*MU" class="com.ucar.actions.MenuAction" method="{1}">
			<result type="json" name="success">
				<param name="root">result</param>
			</result>
			<result name="session_invalid">/pages/session_invalid.jsp</result>
			<result name="list">/pages/menu/list.jsp</result>
			<result name="menulist">/pages/menu/menu_list.jsp</result>
			<result name="error">/pages/error.jsp</result>
			<result name="menuEdit">/pages/menu/menuEdit.jsp</result>
		</action>
	</package>
</struts>
